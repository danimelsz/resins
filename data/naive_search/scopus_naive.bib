@ARTICLE{Bonamigo2017,
author={Bonamigo, T. and Campos, J.F. and Oliveira, A.S. and Torquato, H.F.V. and Balestieri, J.B.P. and Cardoso, C.A.L. and Paredes-Gamero, E.J. and Souza, K.D.P. and Dos Santos, E.L.},
