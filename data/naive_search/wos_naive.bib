@article{ WOS:000369948100002,
Author = {da Cunha, Marcos Guilherme and Rosalen, Pedro Luiz and Franchin, Marcelo
   and de Alencar, Severino Matias and Ikegaki, Masaharu and Ransom, Tanya
   and Beutler, John Albert},
Title = {{Antiproliferative Constituents of Geopropolis from the Bee Melipona
   scutellaris}},
Journal = {{PLANTA MEDICA}},
Year = {{2016}},
Volume = {{82}},
Number = {{3}},
Pages = {{190-194}},
Month = {{FEB}},
Abstract = {{Fractionation of geopropolis from Melipona scutellaris, guided by
   antiproliferative activity against two colon cancer cell lines (COLO205
   and KM12), led to the isolation of two new cinnamic acid esters,
   mammea-type coumarins
   5,7-dihydroxy-6(3-methyl-2-butenyl)-8-(4-cinnamoyl-3-methyl-1-oxobutyl)-
   4-propyl-coumarin (1) and
   5,7-dihydroxy-6-(4-cinnamoyl-3-methyl-1-oxobutyl)-4-phenylcoumarin (2),
   along with five known coumarins, mammeigin (3), hydroxymammeigin (4),
   mammeisin (5), cinnamoyloxy-mammeisin (6), and mammein (7), and the
   prenylated benzophenone ent-nemorosone (8). Among the isolated
   compounds, 5 and 7 showed the highest cell growth inhibition against
   COLO205 (GI(50) 9.7 and 10.7 mu M, respectively) and KM12 (GI(50) 12.0
   and 10.9 mu M, respectively). The presence of these compounds suggests
   that plants of Clusiaceae family, especially the genera Kielmeyera and
   Clusia, are likely to be major sources of geopropolis produced by M.
   scutellaris.}},
