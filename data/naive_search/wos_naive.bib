@article{ WOS:000181169300050,
Author = {Teixeira, ACP and Marini, MM and Nicoli, JR and Antonini, Y and Martins,
   RP and Lachance, MA and Rosa, CA},
Title = {{Starmerella meliponinorum sp nov., a novel ascomycetous yeast species
   associated with stingless bees}},
Journal = {{INTERNATIONAL JOURNAL OF SYSTEMATIC AND EVOLUTIONARY MICROBIOLOGY}},
Year = {{2003}},
Volume = {{53}},
Number = {{1}},
Pages = {{339-343}},
Month = {{JAN}},
Abstract = {{Thirty-two strains of the novel species Starmerella meliponinorum sp.
   nov. were isolated from various substrates associated with three
   stingless bee species (tribe Meliponini) in Brazil and one in Costa
   Rica. The strains were found in garbage pellets (faecal material,
   discarded pollen or food, etc.), pollen provisions, adult bees, honey
   and propolis of Tetragonisca angustula, in honey from Melipona
   quadrifasciata and in adults of Melipona rutiventris and Trigona
   fulviventris. The sequence of the D1/D2 domains of the large-subunit
   rDNA showed that the novel species belongs to the Starmerella clade and
   is most closely related to Candida etchellsii, although the two differ
   in their sequences by 7\% base substitutions. S. meliponinorum is
   homothallic and assimilates few carbon sources. Nitrate is utilized as a
   sole nitrogen source. The type strain of S. meliponinorum is strain
   UFMG-01-J26.1(T) (=CBS 9117(T)).}},
